// Online C compiler to run C program online
#include <stdio.h>
int sum=0;
int main() {
    // Write C code here
     int a[10]={1,2,3,5,7};
    int n=5,t=6;
target(a,n,t);
printf("efefefef");
    return 0;
}

void target(int a[],int n,int t){
    int i;
    if(sum==t)
        {
            printf("sum found\n");
            sum=0;
        }
    else{
    for( i=n-1;i>=0;i--){
    if(can(a[i])){
        sum+=a[i];
        target(a,n-1,t);
        sum-=a[i];
    }    
    }
}
return;
}

int can(int a){
    if(sum+a<=6)
    return 1;
    return 0;
}
